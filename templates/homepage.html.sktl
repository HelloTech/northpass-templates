  <header class="p-relative">

    <img src="{{ homepage.artwork_url }}">

    <div style="top: 0; width: 100%;" class="center-cols expanded p-absolute row text-center m-top-2">
      <div class="small-12 medium-6 large-5 column p-relative m-top-5">
        {{ if homepage.headline }}
          <h1 class="page-header__title">{{ homepage.headline }}</h1>
        {{ end }}
        {{ if homepage.subheadline }}
          <sub class="page-header__subtitle">{{ homepage.subheadline }}</sub>
        {{ end }}

        {{ if current_person }}

          <a href="/courses" class="btn btn-primary m-top-4">
            {{ t('.discover_catalog') }}
          </a>


        {{else}}

          <a href="{{ routes.log_in_path }}" class="btn btn-primary m-top-4">
            Get Started
          </a>
          <a href="{{ routes.log_in_path }}" class="login-link m-top-2">
            Or <span>login</span> to continue learning.
          </a>

        {{end}}


      </div>
    </div>
  </header>

